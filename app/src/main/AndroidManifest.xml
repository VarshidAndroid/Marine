        <activity android:name=".Profile_Screen" />
        <activity android:name=".AboutUs_Screen" />
        <activity android:name=".Notification_Screen" />
        <activity android:name=".Register_Screen" />
        <activity android:name=".OTP" />
        <activity android:name=".Forgot_pass" />
        <activity android:name=".Login_Screen" />
        <activity android:name=".Splash_Screen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>